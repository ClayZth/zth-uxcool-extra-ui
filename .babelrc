{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": false,
        "modules": "commonjs",
        "spec": true,
        "useBuiltIns": "usage",
        "corejs": "2.6.9"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    ["component",{
        "libraryName":"zth-uxcool-extra-ui",
        "libDir":"lib",
        "styleLibrary":{
          "name":"styles",
          "base":false,
          "path":"[module].css"
        }
      }
    ]
  ]
}
